<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".CheckBoxActivity_18">

    <TextView
        android:id="@+id/txt_check_box"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:text="Vote nas novas opções de melhorias" />


    <CheckBox
        android:id="@+id/check_box_menu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/txt_check_box"
        android:layout_marginTop="32dp"
        android:text="Novo Menu" />

    <CheckBox
        android:id="@+id/check_box_layout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/check_box_menu"
        android:layout_marginTop="21dp"
        android:text="Novo Layout" />
</RelativeLayout>